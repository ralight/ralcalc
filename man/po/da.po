msgid "Grundlæggende beregninger: <placeholder-1/>"
msgstr ""

#: ralcalc-da.1.xml:225(userinput)
#, no-wrap
msgid "= -e l 1.0001"
msgstr ""

#: ralcalc-da.1.xml:226(computeroutput)
#, no-wrap
msgid "l 1.0001 = 9.9995e-05"
msgstr ""

#: ralcalc-da.1.xml:224(para)
msgid "Eksponentens nummer uddata format: <placeholder-1/>"
msgstr ""

#: ralcalc-da.1.xml:228(userinput)
#, no-wrap
msgid "= -r l 1.0001"
msgstr ""

#: ralcalc-da.1.xml:229(computeroutput)
#, no-wrap
