msgid "Rå nummer uddata format: <placeholder-1/>"
msgstr ""

#: ralcalc-da.1.xml:231(userinput)
#, no-wrap
msgid "= -s m l 1.0001"
msgstr ""

#: ralcalc-da.1.xml:232(computeroutput)
#, no-wrap
msgid "l 1.0001 = 0.099995"
msgstr ""

