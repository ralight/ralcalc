msgid "ln 1.0001 = 9.9995e-05"
msgstr ""

#: ralcalc-vi.1.xml:234(para)
msgid "Dạng trình bày kết quả số mũ: <placeholder-1/>"
msgstr ""

#: ralcalc-vi.1.xml:238(userinput)
#, no-wrap
msgid "= -r ln 1.0001"
msgstr ""

#: ralcalc-vi.1.xml:239(computeroutput)
#, no-wrap
msgid "ln 1.0001 = 0.000100"
msgstr ""

#: ralcalc-vi.1.xml:237(para)
msgid "Định dạng kết quả số thuần tuý: <placeholder-1/>"
msgstr ""

#: ralcalc-vi.1.xml:241(userinput)
#, no-wrap
msgid "= -s m ln 1.0001"
msgstr ""

#: ralcalc-vi.1.xml:242(computeroutput)
#, no-wrap
msgid "ln 1.0001 = 0.099995"
msgstr ""

