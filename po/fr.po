#: ../src/ralcalc.c:58
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc est une calculatrice en ligne de commande. \n"
"\n"

#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Utilisation; ralcalc -h (affiche ce texte)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a   (affiche tous les opérateurs disponibles)\n"

#: ../src/ralcalc.c:61
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f fichier d'entrée] [-i] [-q] [-r] <une équation>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Options\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e    Utilise la forme d'affichage '1e-3' pour le résultat au lieu des "
"préfixes SI.\n"

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Exécute une liste d'opérations depuis un fichier.\n"

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Execute une liste d'opérations depuis stdin.\n"

#: ../src/ralcalc.c:66
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q N'affiche que le résultat.\n"

#: ../src/ralcalc.c:67
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr ""
" -r Affiche  la réponse avec le préfixe le plus proche SI sans (nor) "
"exposant\n"

#: ../src/ralcalc.c:68
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Allez sur http://atchoo.org/tools/ralcalc/ pour les mises à jour.\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "Numbers:\n"
msgstr "Nombres:\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1.0 : nombre d'exemple\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : nombre d'exemple\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m : nombre d'example\n"

#: ../src/ralcalc.c:83
#, c-format
msgid "  _      : previous result\n"
msgstr "  _: résultat précédent\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  Préfixes SI : y, z, a, f, p, n, u, m\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  Préfixes SI : k, M, G, T, P, E, Z, Y\n"

#: ../src/ralcalc.c:86
#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"Opérateurs :\n"

#: ../src/ralcalc.c:87
#, c-format
msgid "  +     : addition\n"
msgstr "  + :addition\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  -     : subtraction\n"
msgstr "  - :soustraction\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x :multiplication\n"

#: ../src/ralcalc.c:90
#, c-format
msgid "  /     : division\n"
msgstr "  / : division\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %% : modulo\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  ^     : power\n"
msgstr "  ^ : puissance\n"

#: ../src/ralcalc.c:93
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : les parenthèsesaugmentent la précédence\n"

#: ../src/ralcalc.c:94
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Fonctions :\n"

#: ../src/ralcalc.c:95
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X : logarithme en base 10 de X\n"

#: ../src/ralcalc.c:96
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X : logarithme népérien de X\n"

#: ../src/ralcalc.c:97
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : sinus de X, Arcsinus de X\n"

#: ../src/ralcalc.c:98
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : cosinus de X, Arccosinus de X\n"

#: ../src/ralcalc.c:99
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangeante de X, Acrtangeante de X\n"

#: ../src/ralcalc.c:100
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X :racine carrée de X\n"

#: ../src/ralcalc.c:101
#, c-format
msgid ""
"\n"
"Constants:\n"
msgstr ""
"\n"
"Constantes :\n"

#: ../src/ralcalc.c:102
#, c-format
msgid "  pi  : 3.14159\n"
msgstr "  pi : 3.14159\n"

#: ../src/ralcalc.c:103
#, c-format
msgid "  exp : 2.71828\n"
msgstr "  exp : 2.71828\n"

