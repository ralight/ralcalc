#: ../src/ralcalc.c:58
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc is een simpel command-line rekenmachine. \n"
"\n"

#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Gebruik: ralcalc -h (geeft deze tekst weer)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a   (geeft alle mogelijke bewerkingen weer)\n"

#: ../src/ralcalc.c:61
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f invoerbestand] [-i] [-q] [-r] <een vergelijking>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Opties\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Gebruik het '1e-3' formaat van weergave voor het antwoord in plaats van "
"de SI prefixen.\n"

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Verwerk een lijst bewerkingen uit een bestand.\n"

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Verwerk een lijst van berekeningen vanuit stdin.\n"

#: ../src/ralcalc.c:66
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Geef enkel het antwoord weer (stil).\n"

#: ../src/ralcalc.c:67
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r Geeft het antwoord weer zonder SI prefixen, noch exponenten.\n"

#: ../src/ralcalc.c:68
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"http://atchoo.org/tools/ralcalc/ voor updates.\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "Numbers:\n"
msgstr "Nummers:\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1.0 : voorbeeldnummer\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : voorbeeldnummer\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m : voorbeeldnummer\n"

#: ../src/ralcalc.c:83
#, c-format
msgid "  _      : previous result\n"
msgstr "  _ : vorig restultaat\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  y, z, a, f, p, n, u, m : SI prefixen\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  k, M, G, T, P, E, Z, Y : SI prefixen\n"

#: ../src/ralcalc.c:86
#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"Bewerkingen:\n"

#: ../src/ralcalc.c:87
#, c-format
msgid "  +     : addition\n"
msgstr "  + : optelling\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  -     : subtraction\n"
msgstr "  - : aftrekking\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x : vermenigvuldiging\n"

#: ../src/ralcalc.c:90
#, c-format
msgid "  /     : division\n"
msgstr "  / : deling\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %% : restdeling\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  ^     : power\n"
msgstr "  ^ : macht\n"

#: ../src/ralcalc.c:93
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : haakjes veranderen de volgorde van de bewerking\n"

#: ../src/ralcalc.c:94
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Functies:\n"

#: ../src/ralcalc.c:95
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log x : logaritme met basis 10 van X\n"

#: ../src/ralcalc.c:96
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X : natuurlijk logaritme van X\n"

#: ../src/ralcalc.c:97
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : sinus en boogsinus van X\n"

#: ../src/ralcalc.c:98
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : cosinus en boogcosinus van X\n"

#: ../src/ralcalc.c:99
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangens en boogtangens van X\n"

#: ../src/ralcalc.c:100
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X : vierkantswortel van X\n"

#: ../src/ralcalc.c:101
#, c-format
msgid ""
"\n"
"Constants:\n"
msgstr ""
"\n"
"Constanten:\n"

#: ../src/ralcalc.c:102
#, c-format
msgid "  pi  : 3.14159\n"
msgstr "  pi  : 3.14159\n"

#: ../src/ralcalc.c:103
#, c-format
msgid "  exp : 2.71828\n"
msgstr "  exp : 2.71828\n"

