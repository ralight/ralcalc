#: ../src/ralcalc.c:67
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Gebruik: ralcalc -h (geeft deze tekst weer)\n"

#: ../src/ralcalc.c:68
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a   (geeft alle mogelijke bewerkingen weer)\n"

#: ../src/ralcalc.c:69
#, fuzzy, c-format
msgid ""
"       ralcalc [-e] [-f input file] [-i] [-p precision] [-q] [-r] [-s "
"prefix] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f invoerbestand] [-i] [-q] [-r] <een vergelijking>\n"

#: ../src/ralcalc.c:70
#, c-format
msgid "Options\n"
msgstr "Opties\n"

#: ../src/ralcalc.c:71
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Gebruik het '1e-3' formaat van weergave voor het antwoord in plaats van "
"de SI prefixen.\n"

#: ../src/ralcalc.c:72
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Verwerk een lijst bewerkingen uit een bestand.\n"

#: ../src/ralcalc.c:73
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Verwerk een lijst van berekeningen vanuit stdin.\n"

#: ../src/ralcalc.c:74
#, c-format
msgid ""
" -p    Number of digits to display in the output when using -e or -r, or if "
"the\n"
"       output does not have an SI prefix.\n"
msgstr ""

#: ../src/ralcalc.c:75
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Geef enkel het antwoord weer (stil).\n"

#: ../src/ralcalc.c:76
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r Geeft het antwoord weer zonder SI prefixen, noch exponenten.\n"

#: ../src/ralcalc.c:77
#, c-format
msgid " -s    Use the SI prefix 'prefix' to format the output.\n"
msgstr ""

