#: ../src/ralcalc.c:47
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc to prosty kalkulator uruchamiany z wiersza poleceń. \n"
"\n"

#: ../src/ralcalc.c:48
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Wywołanie: ralcalc -h (wyświetla niniejszy tekst)\n"

#: ../src/ralcalc.c:49
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a (wyświetla wszystkie dostępne operatory)\n"

#: ../src/ralcalc.c:50
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr "       ralcalc [-e] [-f plik wejściowy] [-i] [-q] [-r] <równanie>\n"

#: ../src/ralcalc.c:51
#, c-format
msgid "Options\n"
msgstr "Opcje\n"

#: ../src/ralcalc.c:52
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Wyświetlaj wynik w postaci \"1e-3\" zamiast stosować przedrostki SI.\n"

#: ../src/ralcalc.c:53
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Wykonuj obliczenia z listy pobranej z pliku.\n"

#: ../src/ralcalc.c:54
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Wykonuj obliczenia z listy pobranej z wejścia standardowego.\n"

#: ../src/ralcalc.c:55
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Wyświetlaj tylko wynik (tryb cichy).\n"

#: ../src/ralcalc.c:56
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr ""
" -r Wyświetlaj wynik nie używając przedrostków SI ani postaci wykładniczej.\n"

#: ../src/ralcalc.c:57
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Zajrzyj na stronę http://atchoo.org/tools/ralcalc/ w celu aktualizacji.\n"

#: ../src/ralcalc.c:68
#, c-format
msgid "Numbers:\n"
msgstr "Liczby:\n"

#: ../src/ralcalc.c:69
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1,0 : przykładowa liczba\n"

#: ../src/ralcalc.c:70
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1,0e-3 : przykładowa liczba\n"

#: ../src/ralcalc.c:71
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m : przykładowa liczba\n"

#: ../src/ralcalc.c:72
#, c-format
msgid "  _      : previous result\n"
msgstr "  _ : poprzedni wynik\n"

#: ../src/ralcalc.c:73
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  y, z, a, f, p, n, u, m : przedrostki SI\n"

#: ../src/ralcalc.c:74
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  k, M, G, T, P, E, Z, Y : przedrostki SI\n"

#: ../src/ralcalc.c:75
#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"Operatory:\n"

#: ../src/ralcalc.c:76
#, c-format
msgid "  +     : addition\n"
msgstr "  + : dodawanie\n"

#: ../src/ralcalc.c:77
#, c-format
msgid "  -     : subtraction\n"
msgstr "  - : odejmowanie\n"

#: ../src/ralcalc.c:78
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x : mnożenie\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "  /     : division\n"
msgstr "  / : dzielenie\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %% : dzielenie modulo\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  ^     : power\n"
msgstr "  ^ : potęga\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr ""
"  () [] : nawiasy wskazują działania wykonywane w pierwszej kolejności\n"

#: ../src/ralcalc.c:83
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Funkcje:\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X : logarytm o podstawie 10 z liczby X\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X : logarytm naturalny z liczby X\n"

#: ../src/ralcalc.c:86
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : sinus, arcus sinus z liczby X\n"

#: ../src/ralcalc.c:87
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : cosinus, arcus cosinus z liczby X\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangens, arcus tangens z liczby X\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X : pierwiastek kwadratowy z liczby X\n"

#: ../src/ralcalc.c:90
#, c-format
msgid ""
"\n"
"Constants:\n"
msgstr ""
"\n"
"Stałe:\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  pi  : 3.14159\n"
msgstr "  pi : 3,14159\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  exp : 2.71828\n"
msgstr "  exp : 2,71828\n"

