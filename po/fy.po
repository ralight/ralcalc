#: ../src/ralcalc.c:58
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc is in ienfâldige rekkenmesine foar de kommandorigel. \n"
"\n"

#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Gebrûk: ralcalc -h (dizze tekst sjen litte)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a (alle beskikbere operators sjen litte)\n"

#: ../src/ralcalc.c:61
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f ynfoertriem] [-i] [-q] [-r] <in berekkening>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Opsjes\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Brûk de '1e-3'-foarm foar it sjen litten fan it antwurd yn plak fan SI-"
"prefixen.\n"

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Behannelje in list fan berekkeningen út in triem.\n"

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Behannelje in list fan berekkeningen fan stdin.\n"

#: ../src/ralcalc.c:66
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Allinnich it antwurd sjen litte (stil).\n"

#: ../src/ralcalc.c:67
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r It antwurd sjen litte sûnder SI-prefiksen as eksponenten.\n"

#: ../src/ralcalc.c:68
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Sjoch http://atchoo.org/tools/ralcalc/ foar updates.\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "Numbers:\n"
msgstr "Nûmers:\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1.0 : foarbieldnûmer\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : foarbieldnûmer\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m : foarbieldnûmer\n"

#: ../src/ralcalc.c:83
#, c-format
msgid "  _      : previous result\n"
msgstr "  _ : foarich antwurd\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  y, z, a, f, p, n, u, m : SI-prefiksen\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  k, M, G, T, P, E, Z, Y : SI-prefiksen\n"

