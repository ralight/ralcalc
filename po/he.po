#: ../src/ralcalc.c:46
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc הינו מחשבון פשוט לשורת הפקודה. \n"
"\n"

#: ../src/ralcalc.c:47
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "שימוש: ralcalc -h   (הצגת טקסט זה)\n"

#: ../src/ralcalc.c:48
#, c-format
msgid ""
"       ralcalc [-q] [-e] [-r] <an equation>\n"
"\n"
msgstr ""
"       ralcalc [-q] [-e] [-r] <משוואה>\n"
"\n"

#: ../src/ralcalc.c:49
#, c-format
msgid "Options\n"
msgstr "אפשרויות\n"

#: ../src/ralcalc.c:50
#, c-format
msgid " -q\t   Only display the answer (quiet).\n"
msgstr " -q\t   הצג את התשובה בלבד (שקט).\n"

#: ../src/ralcalc.c:51
#, c-format
msgid ""
" -e\t   Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr " -e\t   השתמש בצורה '1e-3' להצגת התשובה מאשר קידומות SI.\n"

#: ../src/ralcalc.c:52
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r    הצג את התשובה בלי קידומות SI או מעריכחפ.\n"

#: ../src/ralcalc.c:53
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"בקרו באתר http://atchoo.org/tools/ralcalc/ לעדכונים.\n"

