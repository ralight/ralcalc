#, c-format
msgid "Error: Invalid SI prefix '%s' for '-s' option.\n"
msgstr "Error: Prefijo SI invalido '%s' for '-s' opcion.\n"

#: ../src/ralcalc.c:371
#, c-format
msgid "Error: Unable to open file \"%s\"\n"
msgstr "Error: Fue imposible abrir el archivo \"%s\"\n"

