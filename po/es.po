#: ../src/ralcalc.c:47
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc es una calculadora simple en linea de comando. \n"
"\n"

#: ../src/ralcalc.c:48
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Uso: ralcalc -h (muestra este mensaje)\n"

#: ../src/ralcalc.c:49
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr ""

#: ../src/ralcalc.c:50
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f fichero de entrada] [-i][-q][-r] <una ecuación>\n"

#: ../src/ralcalc.c:51
#, c-format
msgid "Options\n"
msgstr "Opcciones\n"

#: ../src/ralcalc.c:52
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Usa la notación '1e-3' en lugar de la notación Prefijo SI para mostrar "
"los resultados.\n"

#: ../src/ralcalc.c:53
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Procesa una lista de operaciones apartir de un archivo.\n"

#: ../src/ralcalc.c:54
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr ""
" -i Procesa una lista de operaciones mediante la entrada standar stdin\n"

#: ../src/ralcalc.c:55
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Solo muestra la respuesta (quiet)\n"

#: ../src/ralcalc.c:56
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r Muestra la respuesta sin prefijos SI ni exponentes.\n"

