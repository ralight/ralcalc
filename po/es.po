#: ../src/ralcalc.c:58
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc es una calculadora simple de línea de comandos. \n"
"\n"

#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Uso: ralcalc -h (muestra este mensaje)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a (muestra todos los operadores)\n"

#: ../src/ralcalc.c:61
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f archivo de entrada] [-i][-q][-r] <una ecuación>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Opciones\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Usa la notación '1e-3' en lugar de la notación de prefijo SI para "
"mostrar los resultados.\n"

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Procesa una lista de operaciones apartir de un archivo.\n"

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr ""
" -i Procesa una lista de operaciones mediante la entrada standar stdin\n"

#: ../src/ralcalc.c:66
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Solo muestra la solución (silencioso)\n"

#: ../src/ralcalc.c:67
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r Muestra la solución sin prefijos SI ni exponentes.\n"

#: ../src/ralcalc.c:68
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Visite http://atchoo.org/tools/ralcalc/ para buscar actualizaciones\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "Numbers:\n"
msgstr "Números:\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1.0 : número de ejemplo\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : número de ejemplo\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m : número de ejemplo\n"

#: ../src/ralcalc.c:83
#, c-format
msgid "  _      : previous result\n"
msgstr "  _: solución anterior\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  y, z, a, f, p, n, u, m : prefijos SI\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  k, M, G, T, P, E, Z, Y : prefijos SI\n"

#: ../src/ralcalc.c:86
#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"Operadores:\n"

#: ../src/ralcalc.c:87
#, c-format
msgid "  +     : addition\n"
msgstr "  +: adición\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  -     : subtraction\n"
msgstr "  -: sustracción\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x: producto\n"

#: ../src/ralcalc.c:90
#, c-format
msgid "  /     : division\n"
msgstr "  /: cociente\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %%: módulo\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  ^     : power\n"
msgstr "  ^ : potencia\n"

#: ../src/ralcalc.c:93
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : los paréntesis y corchetes aumentan la precedencia\n"

#: ../src/ralcalc.c:94
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Funciones:\n"

#: ../src/ralcalc.c:95
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X : logaritmo base 10 de X\n"

#: ../src/ralcalc.c:96
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X : logaritmo natural de X\n"

#: ../src/ralcalc.c:97
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : seno, arcoseno de X\n"

#: ../src/ralcalc.c:98
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : coseno, arcocoseno de X\n"

#: ../src/ralcalc.c:99
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangente, arcotangente de X\n"

#: ../src/ralcalc.c:100
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X : raíz cuadrada de X\n"

#: ../src/ralcalc.c:101
#, c-format
msgid ""
"\n"
"Constants:\n"
msgstr ""
"\n"
"Constantes:\n"

#: ../src/ralcalc.c:102
#, c-format
msgid "  pi  : 3.14159\n"
msgstr "  pi: 314159\n"

#: ../src/ralcalc.c:103
#, c-format
msgid "  exp : 2.71828\n"
msgstr "  exp: 271828\n"

