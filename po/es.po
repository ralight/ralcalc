#, c-format
msgid "Error: Invalid SI prefix '%s' for '-s' option.\n"
msgstr "Error: Prefijo SI '%s' inválido para la opción '-s'.\n"

#: ../src/ralcalc.c:371
#, c-format
msgid "Error: Unable to open file \"%s\"\n"
msgstr "Error: Imposible abrir el archivo \"%s\"\n"

