#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Bruk: ralcalc -h   (vis denne teksten)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr ""

#: ../src/ralcalc.c:61
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr "       ralcalc [-e] [-f inndatafil] [-i] [-q] [-r] <en ligning>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Innstillinger\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr ""

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Prosesser en liste av kalkulasjoner fra stdin.\n"

#: ../src/ralcalc.c:66
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q    Vis kun svaret (stille).\n"

#: ../src/ralcalc.c:67
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr ""

