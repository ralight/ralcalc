#: ../src/ralcalc.c:67
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Cách dùng: ralcalc -h   (hiện những dòng chữ này)\n"

#: ../src/ralcalc.c:68
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a   (hiện tất cả các toán tử dùng được)\n"

#: ../src/ralcalc.c:69
#, fuzzy, c-format
msgid ""
"       ralcalc [-e] [-f input file] [-i] [-p precision] [-q] [-r] [-s "
"prefix] <an equation>\n"
msgstr "       ralcalc [-e] [-f file đầu vào] [-i] [-q] [-r] <một biểu thức>\n"

#: ../src/ralcalc.c:70
#, c-format
msgid "Options\n"
msgstr "Tùy chọn\n"

#: ../src/ralcalc.c:71
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr " -e    Dùng dạng '1e-3' thay vì tiền tố SI để hiển thị kết quả.\n"

#: ../src/ralcalc.c:72
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f    Thực hiện một loạt phép tính từ một file.\n"

#: ../src/ralcalc.c:73
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i    Thực hiện một loạt phép tính từ stdin.\n"

#: ../src/ralcalc.c:74
#, c-format
msgid ""
" -p    Number of digits to display in the output when using -e or -r, or if "
"the\n"
"       output does not have an SI prefix.\n"
msgstr ""

#: ../src/ralcalc.c:75
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q    Chỉ hiện kết quả (kiểu im lặng).\n"

#: ../src/ralcalc.c:76
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r    Hiện kết quả mà không có tiền tố SI hoặc số mũ.\n"

#: ../src/ralcalc.c:77
#, c-format
msgid " -s    Use the SI prefix 'prefix' to format the output.\n"
msgstr ""

