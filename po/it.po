#: ../src/ralcalc.c:58
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc è un semplice calcolatore a linea di comando \n"
"\n"

#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Uso: ralcalc -h (mostra questo testo)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a   (mostra gli operatori disponibili)\n"

#: ../src/ralcalc.c:61
#, fuzzy, c-format
msgid ""
"       ralcalc [-e] [-f input file] [-i] [-p precision] [-q] [-r] <an "
"equation>\n"
msgstr "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Opzioni\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Usa la forma '1e-3' per mostrare la risposta invece del prefisso SI\n"

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f    Esegue un alista di computazioni da un file.\n"

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i    Esegue una lista di calcoli da stdin\n"

#: ../src/ralcalc.c:66
#, c-format
msgid ""
" -p    Number of digits to display in the output when using -e or -r, or if "
"the\n"
"       output does not have an SI prefix\n"
"."
msgstr ""

#: ../src/ralcalc.c:67
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q    Mostra soltanto il risultato (quiet).\n"

#: ../src/ralcalc.c:68
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r    Mostra il risultato senza prefisso SI o esponenti.\n"

#: ../src/ralcalc.c:69
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Cosultare http://atchoo.org/tools/ralcalc/ per gli aggiornamenti\n"

