#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"Operatori:\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  +     : addition\n"
msgstr "  +     : addizione\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  -     : subtraction\n"
msgstr "  -     : sottrazione\n"

#: ../src/ralcalc.c:90
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x   : moltiplicazione\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  /     : division\n"
msgstr "  /     : divisione\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %%     : modulo\n"

#: ../src/ralcalc.c:93
#, c-format
msgid "  ^     : power\n"
msgstr "  ^     : elevamento a potenza\n"

#: ../src/ralcalc.c:94
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : parentesi ad incremento di precedenza\n"

#: ../src/ralcalc.c:95
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Funzioni:\n"

#: ../src/ralcalc.c:96
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X         : logaritmo con base 10 di X\n"

#: ../src/ralcalc.c:97
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X          : logaritmo naturale di X\n"

#: ../src/ralcalc.c:98
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : seno, arcoseno di X\n"

#: ../src/ralcalc.c:99
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : coseno, arcocoseno di X\n"

#: ../src/ralcalc.c:100
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangente, arcotangente of X\n"

#: ../src/ralcalc.c:101
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X        : radice quardata di X\n"

