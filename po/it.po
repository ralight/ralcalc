msgstr "  1.0 : numero d'esempio\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : numero d'esempio\n"

#: ../src/ralcalc.c:83
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m   : numero d'esempio\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  _      : previous result\n"
msgstr "  _      : risultato precedente\n"

#: ../src/ralcalc.c:85
#, c-format
