#: ../src/ralcalc.c:58
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc komut satırı ile çalışan basit bir hesap makinesidir. \n"
"\n"

#: ../src/ralcalc.c:59
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Kullanımı: ralcalc -h (bu metni göster)\n"

#: ../src/ralcalc.c:60
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a (tüm mevcut işleçleri görüntüle)\n"

#: ../src/ralcalc.c:61
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f girdi dosyası] [-i] [-q] [-r] <bir denklem>\n"

#: ../src/ralcalc.c:62
#, c-format
msgid "Options\n"
msgstr "Seçenekler\n"

#: ../src/ralcalc.c:63
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e    Cevabı göstermek için SI önekleri yerine '1e-3' biçimini kullan.\n"

#: ../src/ralcalc.c:64
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f    Dosyadan okunan hesaplamalar listesini işle.\n"

#: ../src/ralcalc.c:65
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i stdin'den okunan hesaplamalar listesini işle.\n"

#: ../src/ralcalc.c:66
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Sadece cevapları görüntüle (sessiz).\n"

#: ../src/ralcalc.c:67
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r    Cevabı SI önekleri ve üstel ifadeler olmadan göster.\n"

#: ../src/ralcalc.c:68
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Güncellemeler için http://atchoo.org/tools/ralcalc/ adresini ziyaret edin.\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "Numbers:\n"
msgstr "Sayılar:\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1.0 : örnek sayı\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : örnek sayı\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m : örnek sayı\n"

#: ../src/ralcalc.c:83
#, c-format
msgid "  _      : previous result\n"
msgstr "  _ : önceki sonuç\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  y, z, a, f, p, n, u, m : SI önekleri\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  k, M, G, T, P, E, Z, Y : SI önekleri\n"

#: ../src/ralcalc.c:86
#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"İşleçler:\n"

#: ../src/ralcalc.c:87
#, c-format
msgid "  +     : addition\n"
msgstr "  + : toplama\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  -     : subtraction\n"
msgstr "  - : çıkarma\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x : çarpma\n"

#: ../src/ralcalc.c:90
#, c-format
msgid "  /     : division\n"
msgstr "  / : bölme\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %% : kalan işlemi\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  ^     : power\n"
msgstr "  ^ : üs\n"

#: ../src/ralcalc.c:93
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : parantezler önceliği artırır\n"

#: ../src/ralcalc.c:94
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Fonksiyonlar:\n"

#: ../src/ralcalc.c:95
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X : 10 tabanında X'in logaritması\n"

#: ../src/ralcalc.c:96
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X : X'in doğal logaritması\n"

#: ../src/ralcalc.c:97
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : X'in sinüsü, arksinüsü\n"

#: ../src/ralcalc.c:98
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : X'in kosinüsü, arkkosinüsü\n"

#: ../src/ralcalc.c:99
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : X'in tanjantı, arktanjantı\n"

#: ../src/ralcalc.c:100
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X : X'in kare kökü\n"

#: ../src/ralcalc.c:101
#, c-format
msgid ""
"\n"
"Constants:\n"
msgstr ""
"\n"
"Sabitler:\n"

#: ../src/ralcalc.c:102
#, c-format
msgid "  pi  : 3.14159\n"
msgstr "  pi : 3.14159\n"

#: ../src/ralcalc.c:103
#, c-format
msgid "  exp : 2.71828\n"
msgstr "  exp : 2.71828\n"

