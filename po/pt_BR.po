#: ../src/ralcalc.c:47
#, c-format
msgid ""
"ralcalc is a simple command line calculator. \n"
"\n"
msgstr ""
"ralcalc é uma simples calculadora de linha de comando. \n"
"\n"

#: ../src/ralcalc.c:48
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Uso: ralcalc -h (mostra esse texto)\n"

#: ../src/ralcalc.c:49
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a   (mostrar todas as operações possíveis)\n"

#: ../src/ralcalc.c:50
#, c-format
msgid "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <an equation>\n"
msgstr "       ralcalc [-e] [-f input file] [-i] [-q] [-r] <uma equação>\n"

#: ../src/ralcalc.c:51
#, c-format
msgid "Options\n"
msgstr "Opções\n"

#: ../src/ralcalc.c:52
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr ""
" -e Utilize a forma '1e-3' de visualização do resultado ao invés de usar "
"prefixos SI.\n"

#: ../src/ralcalc.c:53
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Processa uma lista de cálculos a partir do arquivo.\n"

#: ../src/ralcalc.c:54
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Processa uma lista de cálculos a partir da entrada padrão\n"

#: ../src/ralcalc.c:55
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Mostra somente a saída (silencioso).\n"

#: ../src/ralcalc.c:56
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r Mostra a resposta sem os prefixos SI nem expoentes.\n"

#: ../src/ralcalc.c:57
#, c-format
msgid ""
"\n"
"See http://atchoo.org/tools/ralcalc/ for updates.\n"
msgstr ""
"\n"
"Veja http://atchoo.org/tools/ralcalc/ para atualizações.\n"

#: ../src/ralcalc.c:68
#, c-format
msgid "Numbers:\n"
msgstr "Números:\n"

#: ../src/ralcalc.c:69
#, c-format
msgid "  1.0    : example number\n"
msgstr "  1.0    : número de exemplo\n"

#: ../src/ralcalc.c:70
#, c-format
msgid "  1.0e-3 : example number\n"
msgstr "  1.0e-3 : número de exemplo\n"

#: ../src/ralcalc.c:71
#, c-format
msgid "  1.0m   : example number\n"
msgstr "  1.0m   : número de exemplo\n"

#: ../src/ralcalc.c:72
#, c-format
msgid "  _      : previous result\n"
msgstr "  _      :resultado anterior\n"

#: ../src/ralcalc.c:73
#, c-format
msgid "  y, z, a, f, p, n, u, m : SI prefixes\n"
msgstr "  y, z, a, f, p, n, u, m : Prefixos SI\n"

#: ../src/ralcalc.c:74
#, c-format
msgid "  k, M, G, T, P, E, Z, Y : SI prefixes\n"
msgstr "  k, M, G, T, P, E, Z, Y : Prefixos SI\n"

#: ../src/ralcalc.c:75
#, c-format
msgid ""
"\n"
"Operators:\n"
msgstr ""
"\n"
"Operadores:\n"

#: ../src/ralcalc.c:76
#, c-format
msgid "  +     : addition\n"
msgstr "  +     : adição\n"

#: ../src/ralcalc.c:77
#, c-format
msgid "  -     : subtraction\n"
msgstr "  -     : subtração\n"

#: ../src/ralcalc.c:78
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x   : multiplicação\n"

#: ../src/ralcalc.c:79
#, c-format
msgid "  /     : division\n"
msgstr "  /     : divisão\n"

#: ../src/ralcalc.c:80
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %%     : módulo\n"

#: ../src/ralcalc.c:81
#, c-format
msgid "  ^     : power\n"
msgstr "  ^     : expoente\n"

#: ../src/ralcalc.c:82
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : os parênteses aumentam a precedência\n"

#: ../src/ralcalc.c:83
#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Funções:\n"

#: ../src/ralcalc.c:84
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X         : logaritmo de X na base 10\n"

#: ../src/ralcalc.c:85
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X          : logaritmo natural de X\n"

#: ../src/ralcalc.c:86
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : seno, arco do seno de X\n"

#: ../src/ralcalc.c:87
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : cosseno, arco do cosseno de X\n"

#: ../src/ralcalc.c:88
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangente, arco da tangente de X\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X        : raiz quadrada de X\n"

#: ../src/ralcalc.c:90
#, c-format
msgid ""
"\n"
"Constants:\n"
msgstr ""
"\n"
"Constantes:\n"

#: ../src/ralcalc.c:91
#, c-format
msgid "  pi  : 3.14159\n"
msgstr "  pi  : 3.14159\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  exp : 2.71828\n"
msgstr "  exp : 2.71828\n"

