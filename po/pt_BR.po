#, c-format
msgid "  +     : addition\n"
msgstr "  +     : adição\n"

#: ../src/ralcalc.c:89
#, c-format
msgid "  -     : subtraction\n"
msgstr "  -     : subtração\n"

#: ../src/ralcalc.c:90
#, c-format
msgid "  * x   : multiplication\n"
msgstr "  * x   : multiplicação\n"

#: ../src/ralcalc.c:91
#, c-format
