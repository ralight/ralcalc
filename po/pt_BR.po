#, c-format
msgid ""
"\n"
"Functions:\n"
msgstr ""
"\n"
"Funções:\n"

#: ../src/ralcalc.c:96
#, c-format
msgid "  log X         : logarithm to base 10 of X\n"
msgstr "  log X         : logaritmo de X na base 10\n"

#: ../src/ralcalc.c:97
#, c-format
msgid "  ln X          : natural logarithm of X\n"
msgstr "  ln X          : logaritmo natural de X\n"

#: ../src/ralcalc.c:98
#, c-format
msgid "  sin X, asin X : sine, arcsin of X\n"
msgstr "  sin X, asin X : seno, arco do seno de X\n"

#: ../src/ralcalc.c:99
#, c-format
msgid "  cos X, acos X : cosine, arccos of X\n"
msgstr "  cos X, acos X : cosseno, arco do cosseno de X\n"

#: ../src/ralcalc.c:100
#, c-format
msgid "  tan X, atan X : tangent, arctan of X\n"
msgstr "  tan X, atan X : tangente, arco da tangente de X\n"

#: ../src/ralcalc.c:101
#, c-format
msgid "  sqrt X        : square root of X\n"
msgstr "  sqrt X        : raiz quadrada de X\n"

