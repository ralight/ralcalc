msgstr "  /     : divisão\n"

#: ../src/ralcalc.c:92
#, c-format
msgid "  %%     : modulus\n"
msgstr "  %%     : módulo\n"

#: ../src/ralcalc.c:93
#, c-format
msgid "  ^     : power\n"
msgstr "  ^     : expoente\n"

#: ../src/ralcalc.c:94
#, c-format
msgid "  () [] : brackets increase precedence\n"
msgstr "  () [] : os parênteses aumentam a precedência\n"

