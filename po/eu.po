#: ../src/ralcalc.c:67
#, c-format
msgid "Usage: ralcalc -h   (display this text)\n"
msgstr "Erabilera: racalc -h (testu hau erakutsi)\n"

#: ../src/ralcalc.c:68
#, c-format
msgid "       ralcalc -a   (display all available operators)\n"
msgstr "       ralcalc -a (erakutsi eragile eskuragarri guztiak)\n"

#: ../src/ralcalc.c:69
#, fuzzy, c-format
msgid ""
"       ralcalc [-e] [-f input file] [-i] [-p precision] [-q] [-r] [-s "
"prefix] <an equation>\n"
msgstr ""
"       ralcalc [-e] [-f sarrera fitxategia] [-i] [-q] [-r] <ekuazio bat>\n"

#: ../src/ralcalc.c:70
#, c-format
msgid "Options\n"
msgstr "Aukerak\n"

#: ../src/ralcalc.c:71
#, c-format
msgid ""
" -e    Use the '1e-3' form of display for the answer rather than SI "
"prefixes.\n"
msgstr " -e Erabili '1e-3' notazioa SI aurrizkien ordez emaitzak erakusteko.\n"

#: ../src/ralcalc.c:72
#, c-format
msgid " -f    Process a list of calculations from a file.\n"
msgstr " -f Prozesatu eragiketa zerrenda bat fitxategi batetik abiatuz.\n"

#: ../src/ralcalc.c:73
#, c-format
msgid " -i    Process a list of calculations from stdin.\n"
msgstr " -i Prozesatu eragiketa zerrenda bat stdin sarreratik.\n"

#: ../src/ralcalc.c:74
#, c-format
msgid ""
" -p    Number of digits to display in the output when using -e or -r, or if "
"the\n"
"       output does not have an SI prefix.\n"
msgstr ""

#: ../src/ralcalc.c:75
#, c-format
msgid " -q    Only display the answer (quiet).\n"
msgstr " -q Bakarrik emaitza erakutsi (quiet).\n"

#: ../src/ralcalc.c:76
#, c-format
msgid " -r    Display the answer with neither SI prefixes nor exponents.\n"
msgstr " -r Erakutsi emaitza SI aurrizkirik gabe eta berretzailerik gabe.\n"

#: ../src/ralcalc.c:77
#, c-format
msgid " -s    Use the SI prefix 'prefix' to format the output.\n"
msgstr ""

